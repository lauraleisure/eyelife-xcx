/* components/app-nav/app-nav.wxss */
.top-container{
 position: fixed;
  top: 0;
  left: 0;
  right: 0;
  margin: auto;
  z-index: 99999999;
}
.top-nav{
  position: absolute;
  left: 0;
  right: 0;
  margin: auto; 

}
.icon{
  width: 60rpx;
  height: 36rpx;
  line-height: 36rpx;
}
.icon image{
   width: 100%;
   height: 100%;
}
.nav-line{
  width: 1rpx;
  height: 36rpx;
  border-left: 4rpx solid #868686;
  margin: 0 4rpx;
}
.icon-home{
   width: 50rpx;
  height: 40rpx;
  margin: 0 10rpx;
  
}
.nav-title{
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  text-align: center;
}
.flex-box{
  display: flex;
}
.jc-s{
  justify-content: space-between;
}
.jc-st{
  justify-content: flex-start;
}
.ai-c{
  align-items: center;
}